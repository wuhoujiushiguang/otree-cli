{
  "name": "otree-cli",
  "version": "0.0.10",
  "description": "Generate directory tree",
  "main": "lib/otree.js",
  "scripts": {
    "lint": "eslint . --ext '.js' --cache",
    "fix": "eslint . --fix --ext '.js'"
  },
  "repository": {
    "url": "https://github.com/xpromise/otree"
  },
  "bin": {
    "otree": "bin/otree.js"
  },
  "keywords": [
    "directory",
    "tree"
  ],
  "author": "xpromise",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint . --ext '.js' --cache"
    ]
  },
  "dependencies": {
    "yargs": "^15.3.0"
  }
}
